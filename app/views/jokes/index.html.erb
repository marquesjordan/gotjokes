<div class="header">
<h1>Listing all jokes</h1>
</div>

<div class="nav">
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
</div>

<div class="section">
  <tbody>
    <% @jokes.each do |joke| %>
      <tr>
        <td><%= joke.name %></td>
        <td><%= link_to 'Show', joke %></td>
        <td><%= link_to 'Edit', edit_joke_path(joke) %></td>
        <td><%= link_to 'Destroy', joke, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

<%= link_to 'New Joke', new_joke_path %>