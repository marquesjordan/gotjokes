<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Name </th>
      <th>Videos </th>
      <th>Image </th>
      <th>Location </th>
      <th>Biography </th>
      <th colspan="5"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |p| %>
      <tr>
        <td><%= p.name %></td>
        <td><%= p.videos %></td>
        <td><%= p.image %></td>
        <td><%= p.location %></td>
        <td><%= p.bio %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      
      </tr>
    <% end %>
  </tbody>
</table>

<br>

