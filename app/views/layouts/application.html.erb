<!DOCTYPE html>
<html>

<head>
  <title>Gotjokes</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <link href="//vjs.zencdn.net/4.10/video-js.css" rel="stylesheet">
  <script src="//vjs.zencdn.net/4.10/video.js"></script>
</head>


<body ng-app="myApp">

  <!-- start of nav bar -->
  <nav class="navbar navbar-default" role="navigation">
    <div class="container-fluid">
      <div class="navbar-header navbar-brand" >
        <%= link_to image_tag("logo1.jpeg"), root_path, method: :get %>
      </div>
      <div class="navbar-header">
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
          <ul class="nav navbar-nav">
            <li><%= link_to "Jokes", root_path %></li>
            <li><%= link_to "Other", root_path %></li>
            <li class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown">Catagories <span class="caret"></span></a>
              <ul class="dropdown-menu" role="menu">
                <li><%= link_to "Dirty", root_path %></li>
                <li><%= link_to "Clean",  root_path %></li>
                <li><%= link_to "Mama", root_path %></li>
              </ul>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </nav>
  <!-- end of nav bar -->




<a ui-sref="home">HOME</a>
<a ui-sref="show">SHOW</a>
<a ui-sref="list">JOKES LIST</a>




    <div id="container">
        
        <% flash.each do |name,msg| %>
            <%= content_tag :div, msg, id: "flash_#{name}" %>
        <% end %>
    
    <div id="user_header">
    <% if current_user %>
      Logged in as <%= current_user.email %>.
      <%= link_to "Log Out", logout_path %>
    <% else %>
      <%= link_to "Sign Up", new_user_path %> or
      <%= link_to "Log In", new_session_path %>
    <% end %>
    </div>
     
        
    </div>

<%= yield %>

</body>
</html>
